<%= form_with model: @user do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username%><br>
  <%= form.label :email%>
  <%= form.text_field :email%><br>
  <%= form.label :password%>
  <%= form.password_field :password%><br>
  <%= form.submit :Submit%>
<% end %>